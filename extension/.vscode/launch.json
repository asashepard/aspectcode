{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Aspect Code Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "preLaunchTask": "Aspect Code: Build Extension"
    }
  ]
}
